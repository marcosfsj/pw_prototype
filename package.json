{
  "name": "pw_prototype",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "@material-ui/styles": "^4.1.1",
    "axios": "^0.19.0",
    "env-cmd": "^9.0.3",
    "lodash": "^4.17.11",
    "material-table": "^1.39.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "mock:data": "json-server --watch ./src/mock-data/mock-data.json --port 3001",
    "react:start:dev": "env-cmd -e dev react-scripts start",
    "react:build:dev": "env-cmd -e dev react-scripts build",
    "react:start:qa": "env-cmd -e qa react-scripts start",
    "react:build:qa": "env-cmd -e qa react-scripts build",
    "react:start:prod": "env-cmd -e prod react-scripts start",
    "react:build:prod": "env-cmd -e prod react-scripts build",
    "start:dev": "concurrently --kill-others \"npm run mock:data\" \"npm run react:start:dev\"",
    "start:qa": "concurrently --kill-others \"npm run mock:data\" \"npm run react:start:qa\"",
    "test": "env-cmd -e dev react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "json-server": "^0.15.0",
    "moxios": "^0.4.0"
  }
}
